{
	"name": "venetopizza",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@tailwindcss/ui": "^0.6.2",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"autoprefixer": "^10.0.1",
		"framer-motion": "^2.9.4",
		"node-sass": "^4.14.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.1.0",
		"react-router": "^5.2.0",
		"react-scripts": "4.0.0",
		"react-tilt": "^0.1.4",
		"tailwindcss": "^1.9.6",
		"web-vitals": "^0.2.4",
		"webfontloader": "^1.6.28"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"build:tailwind": "tailwindcss build ./tailwind.css -o src/tailwind.output.css --config tailwind.config.js",
		"prestart": "npm run build:tailwind",
		"prebuild": "npm run build:tailwind"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "none",
		"overrides": [
			{
				"files": ".scss",
				"options": {
					"parser": "scss",
					"singleQuote": true,
					"tabWidth": 4,
					"useTabs": true
				}
			}
		]
	}
}
